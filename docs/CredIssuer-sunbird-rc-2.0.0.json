									"raw": "{\n    \"credential\": {\n        \"@context\": [\n            \"https://www.w3.org/2018/credentials/v1\",\n            {\n                \"@context\": {\n                    \"@version\": 1.1,\n                    \"@protected\": true,\n                    \"id\": \"@id\",\n                    \"schema\": \"https://schema.org/\",\n                    \"UniversityDegreeCredential\": {\n                        \"@id\": \"iri:3425\",\n                        \"@context\": {\n                            \"@version\": 1.1,\n                            \"id\": \"@id\",\n                            \"grade\": \"schema:Text\",\n                            \"programme\": \"schema:Text\",\n                            \"certifyingInstitute\": \"schema:Text\",\n                            \"evaluatingInstitute\": \"schema:Text\"\n                        }\n                    }\n                }\n            }\n        ],\n        \"type\": [\n            \"VerifiableCredential\",\n            \"UniversityDegreeCredential\"\n        ],\n        \"issuer\": \"did:rcw:5d5202e5-c451-47f5-b9b0-4974187c27f9\",\n        \"issuanceDate\": \"2023-02-06T11:56:27.259Z\",\n        \"expirationDate\": \"2025-02-08T11:56:27.259Z\",\n        \"credentialSubject\": {\n            \"type\": \"UniversityDegreeCredential\",\n            \"id\": \"did:rcw:bf99c471-ab63-4e2c-b945-58b81ebedde1\",\n            \"grade\": \"A\",\n            \"programme\": \"BE\",\n            \"certifyingInstitute\": \"Anna\",\n            \"evaluatingInstitute\": \"Anna\"\n        }\n    },\n    \"credentialSchemaId\": \"did:schema:417b223a-ae71-4f3d-badf-cdde4d791513\",\n    \"credentialSchemaVersion\": \"1.0.0\",\n    \"tags\": [\n        \"tag1\",\n        \"tag2\",\n        \"tag3\"\n    ],\n    \"method\": \"cbse\"\n}",
